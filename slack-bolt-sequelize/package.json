{
  "name": "slack-bolt-sequelize",
  "version": "0.1.0",
  "description": "Bolt for JavaScript Installation Store Extension - Sequelize",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "build:clean": "shx rm -rf ./dist ./coverage ./.nyc_output",
    "lint": "eslint --fix --ext .ts src",
    "mocha": "TS_NODE_PROJECT=tsconfig.json nyc mocha --config .mocharc.json \"src/**/*.spec.ts\"",
    "test": "npm run lint && npm run mocha",
    "coverage": "codecov",
    "watch": "npx nodemon --watch 'src' --ext 'ts' --exec npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seratch/slack-bolt-extensions.git"
  },
  "keywords": [
    "Slack"
  ],
  "author": "Kazuhiro Sera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seratch/slack-bolt-extensions/issues"
  },
  "homepage": "https://github.com/seratch/slack-bolt-extensions#readme",
  "dependencies": {
    "@slack/oauth": "^2.4.0",
    "@types/node": "^17.0.16",
    "sequelize": "^6.16.1"
  },
  "devDependencies": {
    "@slack/bolt": "^3.9.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "chai": "^4.3.6",
    "codecov": "^3.8.3",
    "eslint": "^8.8.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-jsdoc": "^37.8.2",
    "eslint-plugin-node": "^11.1.0",
    "mocha": "^9.2.0",
    "nyc": "^15.1.0",
    "shx": "^0.3.4",
    "sinon": "^13.0.1",
    "source-map-support": "^0.5.21",
    "sqlite3": "^4.2.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
